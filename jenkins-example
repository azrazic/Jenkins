pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                echo 'Jenkins zatim povremeno provjerava ima li promjena u repozitoriju.Ako otkrije bilo kakve nove obveze ili ažuriranja, automatski pokreće izgradnju projekta.'
            }
        }
     stage ('build')
{ 
     steps
{
echo 'Dobrodosli '
}
}
stage ('deploy')
{ 
     steps
{
echo 'u moj prvi Jenkins primjer '
}
   }

    }
}
